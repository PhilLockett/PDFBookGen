<VBox alignment="CENTER" prefHeight="693.0" prefWidth="475.0" spacing="8.0" stylesheets="@application.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="phillockett65.PDFBookGen.PrimaryController">
   <children>
      <TitledPane collapsible="false" text="File Names">
         <content>
            <VBox spacing="4.0">
               <children>
                  <GridPane hgap="4.0" vgap="4.0">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Label text="Source Document:" GridPane.halignment="RIGHT" />
                        <TextField GridPane.columnIndex="1" GridPane.columnSpan="2" />
                        <Label text="Output File Name:" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
                        <TextField fx:id="myTextField" onKeyTyped="#myTextFieldKeyTyped" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <Button mnemonicParsing="false" text="Browse..." GridPane.columnIndex="2" GridPane.rowIndex="1" />
                     </children>
                  </GridPane>
               </children>
            </VBox>
         </content>
      </TitledPane>
      <TitledPane collapsible="false" layoutX="30.0" layoutY="137.0" text="Check Boxes and Radio Buttons">
         <content>
            <GridPane hgap="4.0" vgap="4.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <CheckBox fx:id="firstCheckBox" mnemonicParsing="false" onAction="#firstCheckBoxActionPerformed" text="Rotate Reverse Side of Sheet" />
                  <CheckBox fx:id="secondCheckBox" mnemonicParsing="false" onAction="#secondCheckBoxActionPerformed" text="Pad Signature with Blank Pages" GridPane.rowIndex="1" />
                  <CheckBox fx:id="thirdCheckBox" mnemonicParsing="false" onAction="#thirdCheckBoxActionPerformed" text="Third Check Box" GridPane.rowIndex="2" />
                  <RadioButton fx:id="firstRadioButton" mnemonicParsing="false" onAction="#myRadioButtonActionPerformed" text="First Radio Button" GridPane.columnIndex="1">
                     <toggleGroup>
                        <ToggleGroup fx:id="myToggleGroup" />
                     </toggleGroup>
                  </RadioButton>
                  <RadioButton fx:id="secondRadioButton" mnemonicParsing="false" onAction="#myRadioButtonActionPerformed" text="Second Radio Button" toggleGroup="$myToggleGroup" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <RadioButton fx:id="thirdRadioButton" mnemonicParsing="false" onAction="#myRadioButtonActionPerformed" text="Third Radio Button" toggleGroup="$myToggleGroup" GridPane.columnIndex="1" GridPane.rowIndex="2" />
               </children>
            </GridPane>
         </content>
      </TitledPane>
      <TitledPane collapsible="false" text="Selections">
         <content>
            <GridPane hgap="4.0" vgap="4.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="141.9999954223633" minWidth="10.0" prefWidth="141.9999954223633" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="221.59996337890624" minWidth="10.0" prefWidth="183.7999755859375" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="175.00000000000003" minWidth="10.0" prefWidth="103.00002441406247" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Output Paper Size:" GridPane.halignment="RIGHT" />
                  <ChoiceBox fx:id="pageSizeChoiceBox" prefWidth="150.0" GridPane.columnIndex="1" />
                  <Label text="Signature Size:" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
                  <ChoiceBox fx:id="sigSizeChoiceBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label fx:id="sigLabel" text="(4 pages)" GridPane.columnIndex="2" GridPane.rowIndex="1" />
               </children>
            </GridPane>
         </content>
      </TitledPane>
      <TitledPane collapsible="false" text="Page Range">
         <content>
            <GridPane hgap="4.0" vgap="4.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="First Page:" GridPane.halignment="RIGHT" />
                  <Spinner fx:id="firstPageSpinner" GridPane.columnIndex="1" />
                  <Label text="Last Page:" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
                  <Spinner fx:id="lastPageSpinner" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
            </GridPane>
         </content>
      </TitledPane>
      <HBox>
         <children>
            <Label fx:id="statusLabel" text="Ready">
               <font>
                  <Font size="14.0" />
               </font></Label>
            <Region HBox.hgrow="ALWAYS" />
            <Button fx:id="clearDataButton" onAction="#clearDataButtonActionPerformed" text="Clear Data" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
</VBox>
