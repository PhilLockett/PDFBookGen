<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="392.0" prefWidth="703.0" spacing="8.0" stylesheets="@application.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="phillockett65.PDFBookGen.PrimaryController">
   <children>
      <TitledPane collapsible="false" text="File Names">
         <content>
            <VBox spacing="4.0">
               <children>
                  <GridPane hgap="4.0" vgap="4.0">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="144.20001220703125" minWidth="10.0" prefWidth="109.00003051757812" />
                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="230.39998779296874" minWidth="10.0" prefWidth="230.39998779296874" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="258.0000671386719" minWidth="10.0" prefWidth="206.60001220703128" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="158.20001220703125" minWidth="10.0" prefWidth="101.79999999999995" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Label text="Source Document:" GridPane.halignment="RIGHT" />
                        <TextField fx:id="sourceDocumentTextField" editable="false" onKeyTyped="#sourceDocumentTextFieldKeyTyped" GridPane.columnIndex="1" GridPane.columnSpan="2" />
                        <Label text="Output File Name:" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
                        <TextField fx:id="outputFileNameTextField" onKeyTyped="#outputFileNameTextFieldKeyTyped" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <Button fx:id="browseButton" mnemonicParsing="false" onAction="#browseButtonActionPerformed" text="Browse..." GridPane.columnIndex="3" GridPane.halignment="RIGHT" />
                     </children>
                  </GridPane>
               </children>
            </VBox>
         </content>
      </TitledPane>
      <TitledPane collapsible="false" layoutX="30.0" layoutY="137.0" text="Output Content">
         <content>
            <GridPane hgap="4.0" vgap="4.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="206.199951171875" minWidth="10.0" prefWidth="147.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="277.6" minWidth="10.0" prefWidth="180.0000244140625" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="327.9999755859375" minWidth="10.0" prefWidth="286.8000366210937" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="327.9999755859375" minWidth="10.0" prefWidth="120.39996337890625" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <CheckBox fx:id="rotateCheckBox" mnemonicParsing="false" onAction="#rotateCheckBoxActionPerformed" text="Rotate Reverse Side of Sheet" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                  <CheckBox fx:id="padCheckBox" mnemonicParsing="false" onAction="#padCheckBoxActionPerformed" text="Pad Signature with Blank Pages" GridPane.columnIndex="2" GridPane.rowIndex="3" />
                  <Label text="First Page:" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
                  <Label text="Last Page:" GridPane.halignment="RIGHT" GridPane.rowIndex="3" />
                  <Spinner fx:id="firstPageSpinner" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Spinner fx:id="lastPageSpinner" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <Label text="Total Page Count:" GridPane.halignment="RIGHT" GridPane.rowIndex="4" />
                  <Label fx:id="countLabel" text="1" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <Label text="Output Paper Size:" GridPane.halignment="RIGHT" />
                  <ChoiceBox fx:id="paperSizeChoiceBox" prefWidth="150.0" GridPane.columnIndex="1" />
                  <Label text="Signature Size:" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
                  <ChoiceBox fx:id="sigSizeChoiceBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label fx:id="sigLabel" text="(4 pages)" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                  <Button fx:id="generateButton" mnemonicParsing="false" onAction="#generateButtonActionPerformed" text="Generate" GridPane.columnIndex="3" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
               </children>
            </GridPane>
         </content>
      </TitledPane>
      <HBox>
         <children>
            <Label fx:id="statusLabel" text="Ready">
               <font>
                  <Font size="14.0" />
               </font></Label>
            <Region HBox.hgrow="ALWAYS" />
            <Button fx:id="clearDataButton" onAction="#clearDataButtonActionPerformed" text="Clear Data" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
</VBox>
